<?xml version="1.0" encoding="UTF-8" ?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:2.0" version="2.0" srcLang="en-US" trgLang="en-US">
    <file id="gally_user.en">
        <unit id="gally_user_email_reset_password_subject">
            <segment>
                <source>gally.user.email.reset_password.subject</source>
                <target>Password reset for %first_name% %last_name%</target>
            </segment>
        </unit>
        <unit id="gally_user_email_reset_password_main_message">
            <segment>
                <source>gally.user.email.reset_password.main_message</source>
                <target>We have received a request to create or change the password for your account. If you did indeed make this request, please click to %link%. </target>
            </segment>
        </unit>
        <unit id="gally_user_email_reset_password_main_message_link">
            <segment>
                <source>gally.user.email.reset_password.main_message.link</source>
                <target>reset your password</target>
            </segment>
        </unit>
        <unit id="gally_user_email_reset_password_ignore_message">
            <segment>
                <source>gally.user.email.reset_password.ignore_message</source>
                <target>If you did not make this request, you can ignore this email and your password will remain unchanged.</target>
            </segment>
        </unit>
        <unit id="gally_user_email_reset_password_polite_message">
            <segment>
                <source>gally.user.email.reset_password.polite_message</source>
                <target>Thank you,</target>
            </segment>
        </unit>
    </file>
</xliff>
