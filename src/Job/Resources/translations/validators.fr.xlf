<?xml version="1.0" encoding="UTF-8" ?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:2.0" version="2.0" srcLang="en-US" trgLang="en-US">
    <file id="validators.fr">
        <unit id="gally_job_file_unique">
            <segment>
                <source>gally.job.file.unique</source>
                <target>Le fichier téléchargé est déjà utilisé pour un autre job.</target>
            </segment>
        </unit>
        <unit id="gally_job_file_unique">
            <segment>
                <source>gally.job.profile.not_valid</source>
                <target>Le profil "{{ profile }}" n'existe pas pour les jobs de type "{{ type }}".</target>
            </segment>
        </unit>
        <unit id="gally_job_file_not_valid">
            <segment>
                <source>gally.job.file.not_valid</source>
                <target>Un fichier ne peut pas être uploadé pour les jobs de type "export".</target>
            </segment>
        </unit>
        <unit id="gally_job_file_required">
            <segment>
                <source>gally.job.file.required</source>
                <target>Un fichier est requis pour les jobs de type "import".</target>
            </segment>
        </unit>
        <unit id="gally.job.file.mime_types">
            <segment>
                <source>gally.job.file.not_csv</source>
                <target>Le fichier uploadé n'est pas un csv.</target>
            </segment>
        </unit>
    </file>
</xliff>
